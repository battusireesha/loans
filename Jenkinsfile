@Library('mylib')_
pipeline
{
    agent any
    stages
    {
        stage('contdwnld_loans')
        {
            steps
            {
                script
                {
                    cicd.gitdwnld("maven")
                }
            }
        }
        stage('contbuild_loans')
        {
            steps
            {
                script
                {
                    cicd.mavenbuild()
                }
            }
        }
    }
}
